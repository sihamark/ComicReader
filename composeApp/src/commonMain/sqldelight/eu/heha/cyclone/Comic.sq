CREATE TABLE Comic (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    homeUrl TEXT NOT NULL,
    title TEXT NOT NULL,
    author TEXT NOT NULL,
    description TEXT NOT NULL,
    coverImageUrl TEXT NOT NULL
);

getLatestRowId:
SELECT last_insert_rowid();

getById:
SELECT * FROM Comic WHERE id = ?;

getAll:
SELECT * FROM Comic;

insert:
INSERT INTO Comic (homeUrl, title, author, description, coverImageUrl) VALUES (?, ?, ?, ?, ?);